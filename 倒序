int main(){
int n,a,b,c,d;
scanf("%d",&n);                      /*输入整数*/
a=n/1000;                            /*取得千位数字*/  
b=n%1000/100;                        /*取得百位数字*/
c=n%100/10;                          /*取得十位数字*/
d=n%10;                              /*取得个位数字*/
printf("%d%d%d%d",d,c,b,a);
return 0;
}
